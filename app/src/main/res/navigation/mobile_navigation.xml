<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.thanhtung.mp3downloader.ui.online.OnlineMusicFragment"
        android:label="@string/title_online"
        tools:layout="@layout/fragment_online_search_music" />
    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.thanhtung.mp3downloader.ui.youtube.YoutubeFragment"
        android:label="@string/title_youtube"
        tools:layout="@layout/fragment_offline_music" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.thanhtung.mp3downloader.ui.offline.OfflineMusicFragment"
        android:label="@string/title_offline"
        tools:layout="@layout/fragment_youtube" />
</navigation>